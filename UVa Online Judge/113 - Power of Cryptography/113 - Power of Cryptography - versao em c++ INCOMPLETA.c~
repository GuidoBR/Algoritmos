#include <iostream>
#include <cmath> // ou math.h
#include <iomanip> // para setprecision
using namespace std;
int main() 
{   // p = base | n = raiz | k = resultado
	double n, p;   
	while(cin >> n >> p) cout << setprecision(1) << pow(p, 1/n) << endl;
    return 0;
}
